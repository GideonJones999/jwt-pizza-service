{
  "collectCoverage": true,
  "coverageReporters": ["json-summary", "text"],
  "verbose": false,
  "testMatch": ["**/test/**/*.test.js"],
  "collectCoverageFrom": ["src/**/*.js", "!src/test/**"],
  "coverageThreshold": {
    "global": {
      "branches": 80,
      "functions": 80,
      "lines": 80,
      "statements": 80
    }
  }
}
